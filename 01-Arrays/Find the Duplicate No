class Solution {
    public int findDuplicate(int[] nums) {
        //hare tortoise method
        int slow = nums[0];
        int fast = nums[0];

        do{
            slow = nums[slow];
            fast = nums[nums[fast]];hh
        }while(slow != fast);

        fast = nums[0];

        while(fast != slow){
            slow = nums[slow];
            fast = nums[fast];
        }

        return fast;
    }
}
